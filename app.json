{
  "expo": {
    "name": "FlixList",
    "slug": "FlixList",
    "description": "A place to view and save your favorite movies",
    "owner": "frizles",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "flixlist",
    "userInterfaceStyle": "dark",
    "githubUrl": "https://github.com/Ronuhz/FlixList",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#15141F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.frizles.FlixList"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#15141F"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.frizles.FlixList"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1dc33b6a-181c-4736-865d-3c207f9c31d4"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1dc33b6a-181c-4736-865d-3c207f9c31d4"
    }
  }
}
